{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Nitro Dev",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "pnpm",
            "runtimeArgs": ["dev"],
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Expo App",
            "request": "launch",
            "type": "node",
            "cwd": "${workspaceFolder}/apps/app",
            "runtimeExecutable": "pnpm",
            "runtimeArgs": [
                "expo",
                "start",
                "--dev-client"
            ],
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}